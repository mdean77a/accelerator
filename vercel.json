{
  "functions": {
    "api/src/main.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/src/main.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    }
  },
  "env": {
    "PYTHONPATH": "api/src"
  }
}